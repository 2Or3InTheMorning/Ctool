        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "ajax-request": "^1.2.3",
        "code-formatter": "0.0.1",
        "cron-parser": "^2.12.0",
        "cronstrue": "^1.72.0",
        "crypto-js": "^3.1.9-1",
        "diff": "^4.0.1",
        "ipinyinjs": "^1.0.0",
        "is-url": "^1.2.4",
        "iview": "^3.4.0",
        "js-base64": "^2.5.1",
        "lscache": "^1.3.0",
        "php-array-reader": "^1.2.0",
        "phparr": "^0.2.0",
        "qrcode": "^1.3.3",
        "qrcode-parser": "^1.2.0",
        "query-string": "^6.6.0",
        "radix.js": "0.0.1",
        "serialize-php": "^1.1.2",
        "vue": "^2.6.10",
